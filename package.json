{
  "name": "r4c-cesium-viewer",
  "private": true,
  "version": "1.30.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- vite --host",
    "dev:test": "VITE_E2E_TEST=true dotenvx run -- vite --host",
    "build": "NODE_OPTIONS='--max-old-space-size=3072' dotenvx run -- vite build",
    "build:analyze": "NODE_OPTIONS='--max-old-space-size=3072' dotenvx run -- vite build --mode analyze",
    "preview": "dotenvx run -- vite preview --host",
    "lint": "eslint --fix src/",
    "lint:deps": "knip",
    "docs": "typedoc",
    "docs:serve": "npm run docs && npx http-server docs -p 8080 -o",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:performance": "vitest run tests/performance",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:accessibility": "playwright test tests/e2e/accessibility",
    "test:accessibility:desktop": "playwright test tests/e2e/accessibility --project=accessibility-desktop",
    "test:accessibility:tablet": "playwright test tests/e2e/accessibility --project=accessibility-tablet",
    "test:accessibility:mobile": "playwright test tests/e2e/accessibility --project=accessibility-mobile",
    "test:accessibility:all-viewports": "npm run test:accessibility:desktop && npm run test:accessibility:tablet && npm run test:accessibility:mobile",
    "test:accessibility:watch": "playwright test tests/e2e/accessibility --ui",
    "test:accessibility:report": "playwright show-report",
    "test:accessibility:focused": "playwright test tests/e2e/accessibility/ --workers=1 --max-failures=1",
    "test:accessibility:file": "playwright test --workers=1 --max-failures=1",
    "test:layer-controls": "npm run test:accessibility:file tests/e2e/accessibility/layer-controls.spec.ts",
    "test:building-filters": "npm run test:accessibility:file tests/e2e/accessibility/building-filters.spec.ts",
    "test:navigation-levels": "npm run test:accessibility:file tests/e2e/accessibility/navigation-levels.spec.ts",
    "test:expansion-panels": "npm run test:accessibility:file tests/e2e/accessibility/expansion-panels.spec.ts",
    "test:timeline-controls": "npm run test:accessibility:file tests/e2e/accessibility/timeline-controls.spec.ts",
    "test:view-modes": "npm run test:accessibility:file tests/e2e/accessibility/view-modes.spec.ts",
    "test:comprehensive-walkthrough": "npm run test:accessibility:file tests/e2e/accessibility/comprehensive-walkthrough.spec.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:ci:accessibility": "npm run test:accessibility:all-viewports",
    "test:performance:monitor": "PERFORMANCE_MONITORING=true playwright test tests/e2e/accessibility",
    "test:performance:check": "node scripts/check-performance-regression.js",
    "test:performance:baseline": "PERFORMANCE_MONITORING=true playwright test tests/e2e/accessibility && echo 'Baseline generated in tests/performance-baselines.json'"
  },
  "dependencies": {
    "@sentry/browser": "^10.23.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@sentry/vue": "^10.23.0",
    "@turf/turf": "^7.2.0",
    "axios": "^1.13.2",
    "cesium": "^1.135.0",
    "d3": "^7.9.0",
    "fast-xml-parser": "^5.3.1",
    "mitt": "^3.0.1",
    "pinia": "^3.0.4",
    "tiff-imagery-provider": "^2.17.1",
    "unplugin-vue-components": "^30.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.24",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@eslint/js": "^9.39.1",
    "typescript-eslint": "^8.46.3",
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.7",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "knip": "^5.69.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "supertest": "^7.1.4",
    "typedoc": "^0.28.14",
    "unplugin-auto-import": "^0.18.5",
    "vite": "^7.2.2",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-cesium-build": "^0.7.2",
    "vitest": "^4.0.7"
  },
  "overrides": {
    "esbuild": "^0.25.11"
  }
}
