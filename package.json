{
  "name": "r4c-cesium-viewer",
  "private": true,
  "version": "1.27.7",
  "type": "module",
  "scripts": {
    "dev": "./node_modules/.bin/dotenvx run -- vite --host",
    "build": "./node_modules/.bin/dotenvx run -- vite build",
    "preview": "./node_modules/.bin/dotenvx run -- vite preview --host",
    "lint": "eslint --fix src/",
    "docs": "typedoc",
    "docs:serve": "npm run docs && npx http-server docs -p 8080 -o",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:performance": "vitest run tests/performance",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:accessibility": "playwright test tests/e2e/accessibility",
    "test:accessibility:desktop": "playwright test tests/e2e/accessibility --project=accessibility-desktop",
    "test:accessibility:tablet": "playwright test tests/e2e/accessibility --project=accessibility-tablet",
    "test:accessibility:mobile": "playwright test tests/e2e/accessibility --project=accessibility-mobile",
    "test:accessibility:all-viewports": "npm run test:accessibility:desktop && npm run test:accessibility:tablet && npm run test:accessibility:mobile",
    "test:accessibility:watch": "playwright test tests/e2e/accessibility --ui",
    "test:accessibility:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:ci:accessibility": "npm run test:accessibility:all-viewports"
  },
  "dependencies": {
    "@sentry/browser": "^10.22.0",
    "@sentry/vite-plugin": "^4.5.0",
    "@sentry/vue": "^10.22.0",
    "@turf/turf": "^7.2.0",
    "axios": "^1.12.2",
    "d3": "^7.9.0",
    "fast-xml-parser": "^5.3.0",
    "mitt": "^3.0.1",
    "path": "^0.12.7",
    "pinia": "^3.0.3",
    "tiff-imagery-provider": "^2.17.1",
    "typescript-eslint": "^8.46.2",
    "unplugin-vue-components": "^30.0.0",
    "url": "^0.11.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^2.1.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.3",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "supertest": "^7.1.4",
    "typedoc": "^0.28.14",
    "vite": "^7.1.12",
    "vite-plugin-cesium": "^1.2.23",
    "vite-plugin-cesium-build": "^0.7.2",
    "vitest": "^4.0.3"
  },
  "overrides": {
    "esbuild": "^0.25.11"
  }
}
